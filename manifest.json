{
    "name": "Black Lives Matter",
    "version": "1.0",
    "manifest_version": 2,
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "BLM",
        "default_icon": "blm.png"
    }
}